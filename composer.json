{
    "name": "duke/horizon",
    "description": "Telegram bot template for Laravel",
    "license": "MIT",
    "require": {
        "symfony/http-foundation": "^5.0",
        "tightenco/collect": "^8.0",
        "mpociot/pipeline": "^1.0.2",
        "react/socket": "~1.0",
        "spatie/macroable": "^1.0",
        "beyondcode/laravel-dump-server": "^1.7",
        "duke/helper": "1.0.x-dev",
        "spatie/emoji": "^2.3",
        "vinkla/shield": "^7.1"
    },
    "require-dev": {
        "codeigniter/framework": "~3.0",
        "orchestra/testbench": "^6.0",
        "doctrine/cache": "^1.6",
        "symfony/cache": "^5.0",
        "ext-curl": "*"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Duke\\Horizon\\HorizonServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Duke\\Horizon\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    }
}
